{
  "@context": [
    "https://linkedsoftwaredependencies.org/bundles/npm/css-git-http-backend-module/^1.0.0/components/context.jsonld",
    "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^4.0.0/components/context.jsonld",
    {
      "solid-git": "urn:solid-git:default:",
      "solid-server": "urn:solid-server:default:"
    }
  ],

  "@graph": [
    {
      "comment": "Handles a HttpRequest including Git",
      "@id": "css-git-http-backend-module:GitRequestHandler",
      "@type": "GitRequestHandler",
      "GitRequestHandler:_rootFilePath" : "./myData",
      "GitRequestHandler:_gitBackendPath" : "/usr/lib/git-core/git-http-backend",
      "GitRequestHandler:_gitRegex" : "/\/*.git\/\/?($|git-upload-pack|git-receive-pack|info\/refs\\?service=git-upload-pack|info\/refs\\?service=git-receive-pack)"
    }

  ]
}
